|module.run(['$templateCache', ($templateCache) ->
|  $templateCache.put('contrib/hipchat', '''
div(contrib-hipchat-webhooks, ng-controller="ContribHipChatAdminController as ctrl")
    header
        h1(tg-main-title)

    form
        fieldset
            label(for="url") HipChat webhook url
            input(type="text", name="url", ng-model="hipchathook.url", placeholder="HipChat webhook url", id="url", data-type="url")

        button(type="submit", class="hidden")
        a.button.button-green.submit-button(href="", title="Save", ng-click="ctrl.updateOrCreateHook(hipchathook)") Save

    a.help-button(href="https://taiga.io/support/hipchat-integration/", target="_blank")
        span.icon.icon-help
        span Do you need help? Check out our support page!
|''')]);
